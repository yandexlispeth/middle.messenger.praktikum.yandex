.input-text_field {
  box-sizing: border-box;
  text-align: left;
  padding-left: 14px;
  background: #e7e8ec;
  font-style: normal;
  font-weight: 500;
  font-size: 16px;
  font-family: "Inter";
  width: 100%;
  height: 34px;
  border: 0px;
  border-radius: 10px;
}

.input-text_field::placeholder {
  opacity: 0.5;
  color: #aeabab;
  font-family: "Titillium Web";
  font-style: normal;
  font-weight: 400;
  font-size: 16px;
  line-height: 21px;
}